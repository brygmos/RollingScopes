{
  "name": "testlint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "ts-node --esm server.ts",
    "server:cypress": "start-server-and-test server http://localhost:3000 cypress:run",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "prettier": "prettier --write .",
    "test": "vitest --coverage",
    "test:ui": "vitest --ui",
    "build": "tsc && vite build",
    "buildssr": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "cypress:open": "cypress open",
    "cypress:panel": "cypress open",
    "report": "npx nyc report --reporter=text-summary",
    "cypress": "npx cypress run --e2e && npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:run": "cypress run --env coverage=true && npx nyc report --reporter=text-summary",
    "cypress:coverage": "npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/jest": "^29.4.4",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.10.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  }
}
